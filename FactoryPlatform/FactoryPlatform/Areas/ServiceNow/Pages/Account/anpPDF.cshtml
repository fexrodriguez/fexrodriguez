                                                <td class="align-middle p-2 bg-gray-100 fs-6" colspan="7" scope="colgroup">@Model.ANP.DigitalDocumentsANP.Where(A => A.TipoID == 3).First().Title: </td>
                                            </tr>
                                            <tr class="border border-top border-bottom border-end border-start border-gray-900 text-gray-900  bg-gray-300  fs-5">
                                                <th class="p-2 align-middle w-50px">#</th>
                                                <th class="p-2 align-middle w-200px">Parametro</th>
                                                <th class="p-2 align-middle w-200px text-center">Resultado</th>
                                                <th class="p-2 align-middle w-200px text-center">Conclusion</th>
                                            </tr>
                                            @foreach (var ANP in Model.ANP.DigitalDocumentsANP.Where(A => A.TipoID == 3).ToList())
                                            {
                                                <tr class="border border-top border-bottom border-end border-start border-gray-900 fs-6" style="background-color: #@ANP.Color;">
                                                    <td class="p-2 align-middle w-50px">@rowcount</td>
