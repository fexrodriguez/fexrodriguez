                                                    <th class="p-2 align-middle w-300px">@ANP.Descripcion</th>
                                                    <th class="p-2 align-middle w-100px text-center">@ANP.Unidad</th>
                                                    <th class="p-2 align-middle w-100px text-center">@ANP.Minimo</th>
                                                    <th class="p-2 align-middle w-100px text-center">@ANP.Maximo</th>
                                                    <th class="p-2 align-middle w-100px text-center">@ANP.Medicion</th>
                                                    <th class="p-2 align-middle w-100px text-center">@ANP.Conclusion</th>
