@page "/ServiceNow/anpPDF"
@model FactoryPlatform.Areas.ServiceNow.Pages.Account.anpPDFModel
@{
}
<html lang="en">
<!--begin::Head-->
<head>
    <base href="../../">
    <title>FactoryPlatform - @ViewData["Title"]</title>
    <meta name="viewport" content="width=device-width, initial-scale=1" />
    <meta charset="utf-8" />
    <link rel="shortcut icon" href="~/Images/SystemPictures/favicons.png" />
    <!--begin::Fonts-->
    <link rel="stylesheet" href="https://fonts.googleapis.com/css?family=Poppins:300,400,500,600,700" />
    <!--end::Fonts-->
    <!--begin::Global Stylesheets Bundle(used by all pages)-->
    <link href="~/assets/plugins/global/plugins.bundle.css" rel="stylesheet" type="text/css" />
    <link href="~/assets/css/style.bundle.css" rel="stylesheet" type="text/css" />
    <!--end::Global Stylesheets Bundle-->
</head>
<!--end::Head-->
<!--begin::Body-->
<body id="kt_body" class="bg-body" background="~/Images/SystemPictures/newLogo3.png">

    @if (Model.ANP != null)
    {
        <div class="d-flex flex-column flex-root">
            <div class="d-flex flex-column flex-column-fluid">
                <div class="d-flex flex-column flex-column-fluid p-10 py-lg-15">
                    <!-- begin::Wrapper-->
                    @if (Model.ANP.ANP_Signatures.Count > 0)
                    {
                        var AnpSignatures = Model.ANP.ANP_Signatures.First();

                        <div class="mw-lg-950px mx-auto w-100">
                            <table class="table border border-gray-900 gy-7 gs-7 gx-7 mb-10">
                                <tr class=" border-start border-bottom border-top border-end border-gray-900  fw-bolder text-gray-900">
                                    <th class="align-middle text-center p-3 bg-gray-300" colspan="6" scope="colgroup">
                                        <h1 class="card-title fs-1qx text-center">VEGE DE MEXICO S.A. DE C.V.</h1>
                                    </th>
                                </tr>
                                <tr class=" border-bottom border-end border-gray-900 text-gray-900  fs-6 p-2">
                                    <td class="p-2 border-start  border-gray-900  w-200px align-middle text-center">DEPARTAMENTO:</td>
                                    <td class="p-2 w-300px align-middle text-center"> INGENIERIA (77)</td>
                                    <td class="p-2 w-200px align-middle text-center">EDICION:</td>
                                    <td class="p-2 w-200px align-middle text-center">1</td>
                                    <td class="p-2   align-middle text-center" colspan="2" rowspan="3">
                                        <img alt="Logo" src="~/Images/SystemPictures/LogoLkqNew.png" class="w-250px" />
                                    </td>
                                </tr>
                                <tr class=" border-bottom border-end border-gray-900 text-gray-900  fs-6 p-2">
                                    <td class="p-2  border-start border-gray-900 w-200px align-middle text-center ">CLAVE:</td>
                                    <td class="p-2  w-300px align-middle text-center">
                                        @{
                                            AnpSignatures.Codigo = AnpSignatures.CodigoMedio == "ANQ" || AnpSignatures.CodigoMedio == "ANHMA" ? $"{AnpSignatures.CodigoMedio} - {AnpSignatures.Codigo}" : $"ANP-{AnpSignatures.Codigo}";
                                        } @AnpSignatures.Codigo
                                    </td>
                                    <td class="p-2  w-200px align-middle text-center">FECHA:</td>
                                    <td class="p-2  w-200px align-middle text-center"> @AnpSignatures.Fecha</td>
                                </tr>
                                <tr class="border border-bottom border-end border-gray-900 text-gray-900 fs-6 p-2">
                                    <th class="p-2  w-200px align-middle text-center ">TIPO:</th>
                                    <td class="p-2  w-300px align-middle text-center"> TODOS</td>
                                    <td class="p-2  w-200px align-middle text-center">ELABORO:</td>
                                    <td class="p-2  w-200px align-middle text-center">@AnpSignatures.Ingeniero</td>
                                </tr>
                                <tr class="border border-bottom border-end border-gray-900 border-gray-900 fw-bolder text-gray-900 ">
                                    @if (AnpSignatures.Aprobado == 1)
                                    {
                                        <th style="background-color: #58FA82;" class="align-middle text-center p-2" colspan="6" scope="colgroup">@AnpSignatures.Codigo - @AnpSignatures.DescripcionCodigoMedio - @AnpSignatures.NombreProveedor - Approved</th>

                                    }
                                    else
                                    {
                                        <th style="background-color: #F78181;" class="align-middle text-center p-2" colspan="6" scope="colgroup">@AnpSignatures.Codigo - @AnpSignatures.DescripcionCodigoMedio - @AnpSignatures.NombreProveedor - Rejected</th>
                                    }
                                </tr>
                                <tr class="border border-bottom border-end border-gray-900 text-gray-900 ">
                                    <th class="align-middle p-2 bg-gray-100 fs-6" colspan="6" scope="colgroup">ANP Request Information:</th>
                                </tr>
                                <tr class="border border-bottom border-end border-gray-900 text-gray-900  fs-6 ">
                                    <td class="p-2 align-middle w-200px fw-bolder">Requested by:</td>
                                    <td class="p-2 align-middle w-300px  text-center">@AnpSignatures.Solicito</td>
                                    <td class="p-2 align-middle w-200px fw-bolder">Supplier:</td>
                                    <td class="p-2 align-middle w-100px  text-center">(@AnpSignatures.ClaveProveedor) @AnpSignatures.NombreProveedor</td>
                                    <td class="p-2 align-middle w-200px fw-bolder">Item Code:</td>
                                    <td class="p-2 align-middle w-200px  text-center">@AnpSignatures.Codigo</td>
                                </tr>
                                <tr class="border border-bottom border-end border-gray-900 text-gray-900  fs-6 ">
                                    <td class="p-2 align-middle w-200px fw-bolder">Part Number Item-Supplier:</td>
                                    <td class="p-2 align-middle w-300px text-center">@AnpSignatures.NumeroParte</td>
                                    <td class="p-2 align-middle w-200px fw-bolder">Job:</td>
                                    <td class="p-2 align-middle w-100px  text-center">@AnpSignatures.NumeroJob</td>
                                    <td class="p-2 align-middle w-200px fw-bolder">Approval Reason:	</td>
                                    <td class="p-2 align-middle w-200px text-center">@AnpSignatures.Razon</td>
                                </tr>
                                <tr class="border border-bottom border-end border-gray-900 text-gray-900  fs-6 ">
                                    <td class="p-2 align-middle w-200px fw-bolder">Quality Fails(last year):</td>
                                    <td class="p-2 align-middle w-300px text-center">@AnpSignatures.FallasCalidad</td>
                                    <td class="p-2 align-middle w-200px fw-bolder">Comments to Approval Reason:</td>
                                    <td class="p-2 align-middle w-100px text-center">@AnpSignatures.Comentarios</td>
                                    <td class="p-2 align-middle w-200px fw-bolder">Type File:</td>
                                    <td class="p-2 align-middle w-200px text-center">@AnpSignatures.TipoArchivo</td>
                                </tr>
                                <tr class="border border-bottom border-end border-gray-900 text-gray-900  fs-6 ">
                                    <td class="p-2 align-middle w-200px fw-bolder">Request:	</td>
                                    <td class="p-2 align-middle w-300px text-center">REQ-@AnpSignatures.ID_Request</td>
                                    <td class="p-2 align-middle w-200px fw-bolder">Aplication:	</td>
                                    <td class="p-2 align-middle w-100px text-center">@AnpSignatures.DescripcionAplicacion</td>
                                    <td class="p-2 align-middle w-200px fw-bolder">Department:	</td>
                                    <td class="p-2 align-middle w-200px text-center">77</td>
                                </tr>
                                <tr class="border  border-end border-gray-900 text-gray-900  fs-6 ">
                                    <td class="p-2 border-bottom border-gray-900  align-middle w-200px fw-bolder">Standard Cost:	</td>
                                    <td class="p-2 border-bottom border-gray-900 align-middle w-300px text-center"> $ @AnpSignatures.CostoStandar</td>
                                    <td class="p-2 border-bottom border-gray-900 align-middle w-200px fw-bolder">New Cost:</td>
                                    <td class="p-2 border-bottom border-gray-900 align-middle w-100px text-center"> $ @AnpSignatures.NuevoPrecio</td>
                                    <td class="p-2 border-bottom border-gray-900 align-middle w-200px fw-bolder">Consumption Monthly:	</td>
                                    <td class="p-2 border-bottom border-gray-900 align-middle w-200px text-center"> $ @AnpSignatures.ConsumoAnual</td>
                                </tr>
                            </table>

                            @if (Model.ANP.DigitalDocumentsANP.Count > 0)
                            {

                                <div class="border border-bottom  border-gray-900 p-2 mb-10">

