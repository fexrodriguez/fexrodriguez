                                                    <td class="p-2 align-middle w-50px">@rowcount</td>
                                                    <td class="p-2 align-middle w-200px">@ANP.Descripcion</td>
                                                    <td class="p-2 align-middle w-200px">@ANP.Resultado</td>
                                                    <td class="p-2 align-middle w-200px">@ANP.Conclusion</td>
                                                </tr>
                                                rowcount++;
                                            }
                                        </table>
                                    }

                                    @*-----------------------------------------------------------------------------------------------
                                    -- 3.- Comments (General Inspection)
                                    -----------------------------------------------------------------------------------------------*@
                                    @if (Model.ANP.DigitalDocumentsANP.Where(A => A.TipoID == 5).ToList().Count > 0)
                                    {
                                        <table class="table border border-gray-900 gy-7 gs-7 gx-7 mb-10">
                                            <tr class="border border-top border-bottom border-end border-start border-gray-900 text-gray-900  ">
                                                <td class="align-middle p-2 bg-gray-100 fs-6" colspan="7" scope="colgroup">@Model.ANP.DigitalDocumentsANP.Where(A => A.TipoID == 5).First().Title: </td>
                                            </tr>
                                            <tr class="border border-top border-bottom border-end border-start border-gray-900 fs-6">
                                                <td class="p-2 align-middle"> @AnpSignatures.FComentarios</td>
                                            </tr>
                                        </table>
                                    }

